{#pageNavigation}
	<section class="pageNavigation">
		{#goToPage}
			{>textField /}
		{/goToPage}
		<a class="goToPage" href="#">Go To Page</a>
		<figure class="pageNumbering">
			{page} <span class="currentResultPage">{currentPage}</span> {of} {totalPages}
